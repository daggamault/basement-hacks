{
  "name": "web-shared-utils",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/web/shared/utils/src",
  "prefix": "bh",
  "tags": ["web", "utils"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/web/shared/utils/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/web/shared/utils/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/web/shared/utils/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/web/shared/utils/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
