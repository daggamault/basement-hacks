{
  "name": "api-shared-utils",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/shared/utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/shared/utils",
        "tsConfig": "libs/api/shared/utils/tsconfig.lib.json",
        "packageJson": "libs/api/shared/utils/package.json",
        "main": "libs/api/shared/utils/src/index.ts",
        "assets": ["libs/api/shared/utils/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/api/shared/utils/jest.config.ts"
      }
    }
  },
  "tags": ["api", "utils"]
}
